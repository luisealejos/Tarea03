<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tarea03"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar" >
        <activity
            android:name="com.example.tarea03.activities.MainActivity"
            android:label="@string/app_name" >
			<intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.tarea03.activities.ListStoreActivity"
            android:label="@string/title_activity_list_store" >
        </activity>
        <activity
            android:name="com.example.tarea03.activities.InfoStoreActivity"
            android:label="@string/title_activity_info_store" >
        </activity>
        <activity
            android:name="com.example.tarea03.activities.ImageViewActivity"
            android:label="@string/title_activity_image_view" >
        </activity>
    </application>

</manifest>
